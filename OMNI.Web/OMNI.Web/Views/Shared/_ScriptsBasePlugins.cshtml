<script type="text/javascript">
    var base_api = "@Context.Request.Scheme://@Context.Request.Host/";

    var isManagement = "@ViewBag.Roles.Contains("OSMOSYS_MANAGEMENT")";
    console.log("status: " + isManagement);

    $(document).on('click', `a[data-toggle="modal"]`, function () {
        let elA = $(this);
        if (elA.attr("href") !== "#") {
            $(`${$(elA).data("target")} .modal-content`).load($(this).attr("href"));
        }
    });

    // menu multi app
    $(document).on('click', '.nav-custom .nav-item', function () {
        $('.nav-custom .nav-item').removeClass('active');
        $(this).addClass('active');
    })
</script>